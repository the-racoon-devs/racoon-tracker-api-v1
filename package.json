{
    "name": "racoon-tracker-api-v1",
    "version": "1.0.0",
    "description": "The API for Racoon Tracker",
    "main": "index.ts",
    "scripts": {
        "build": "tsc",
        "start": "nodemon ./dist/index.js",
        "dev": "nodemon --exec ts-node -- ./src/index.ts",
        "prod": "npm run build && npm run start",
        "clean": "rm -rf node_modules coverage dist .nyc_output",
        "lint": "tslint -t stylish --project \"tsconfig.json\""
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/the-racoon-devs/racoon-tracker-api-v1.git"
    },
    "author": "Fabian Ferno",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/the-racoon-devs/racoon-tracker-api-v1/issues"
    },
    "homepage": "https://github.com/the-racoon-devs/racoon-tracker-api-v1#readme",
    "dependencies": {
        "body-parser": "^1.18.3",
        "cors": "^2.8.5",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "express-jwt": "^6.1.0",
        "helmet": "^4.6.0",
        "http-status": "^1.2.0",
        "jwks-rsa": "^2.0.5",
        "mongodb": "4.2.2",
        "morgan": "^1.10.0",
        "nocache": "^3.0.1"
    },
    "devDependencies": {
        "@types/dotenv": "^4.0.3",
        "@types/express": "^4.16.0",
        "@types/node": "^10.9.4",
        "morgan": "^1.9.1",
        "nodemon": "2.0.15",
        "ts-node": "^10.4.0",
        "tslint": "^5.11.0",
        "tslint-config-prettier": "^1.15.0",
        "typescript": "^4.5.4"
    },
    "nodemonConfig": {
        "ext": "js,json,ejs,jpg,png,gif,svg,css,html,ts,env",
        "watch": [
            "src",
            "dist"
        ]
    },
    "optionalDependencies": {
        "fsevents": "2.3.2"
    }
}